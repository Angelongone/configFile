        "commands": [":sp"]
      },
      {
        "before": ["<leader>","r"],
        "commands": [":vsp"]
      },
      {
        "before": ["<leader>","p"],
        "commands": [":tabp"]
      },
      {
        "before": ["<leader>","n"],
        "commands": [":tabn"]
      },
      {
        "before": ["<leader>","o"],
        "commands": [":tabo"]
      },
      {
        "before": ["<leader>","s"],
        "commands": [":tabs"]
      },
      {
        "before": ["<leader>","c"],
        "commands": [":tabc"]
      },
      {
        "before": ["<leader>","e","w"],
